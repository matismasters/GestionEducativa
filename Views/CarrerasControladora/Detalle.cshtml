@{
}

<a href="/CarrerasControladora">Volver al Listado</a>

<h1>Detalle de Carrera @ViewBag.Carrera.Nombre</h1>

<p>Descripcion: @ViewBag.Carrera.Descripcion</p>

<p>Requerimientos: @ViewBag.Carrera.Requerimientos</p>

<p>Semestres:</p>
<ul>
    @foreach (Semestre semestre in ViewBag.Carrera.Semestres)
    {
        <li>@semestre.Nombre</li>
    }
</ul>